{
  "name": "remix-app-cloudflare",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix build",
    "clean": "rm -Rf node_modules/ build/ .cache/",
    "db:generate": "drizzle-kit generate:sqlite",
    "db:dev": "wrangler d1 execute DB --local",
    "db:preview": "wrangler d1 execute DB --preview",
    "db:production": "wrangler d1 execute DB",
    "dev": "remix dev --manual -c \"bun run start\"",
    "format": "bun biome format --write ./app",
    "lint": "bun biome check --apply ./app",
    "start": "wrangler pages dev --compatibility-date=2024-04-04 ./public",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@lucia-auth/adapter-drizzle": "1.0.0",
    "@lucia-auth/adapter-sqlite": "3.0.0",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-scroll-area": "^1.0.5",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@react-email/components": "0.0.16",
    "@react-email/render": "^0.0.12",
    "@remix-run/cloudflare": "^2.8.1",
    "@remix-run/cloudflare-pages": "^2.8.1",
    "@remix-run/css-bundle": "^2.8.1",
    "@remix-run/react": "^2.8.1",
    "@remix-run/serve": "^2.8.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "cmdk": "^1.0.0",
    "date-fns": "^3.6.0",
    "drizzle-orm": "^0.30.7",
    "framer-motion": "^11.0.25",
    "isbot": "^5.1.4",
    "lucia": "3.0.1",
    "lucide-react": "^0.365.0",
    "react": "^18.2.0",
    "react-day-picker": "^8.10.0",
    "react-dom": "^18.2.0",
    "recharts": "^2.12.4",
    "tailwind-merge": "^2.2.2",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^0.9.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.6.4",
    "@cloudflare/ai": "^1.1.0",
    "@cloudflare/workers-types": "^4.20240405.0",
    "@remix-run/dev": "^2.8.1",
    "@tauri-apps/cli": "^1.5.11",
    "@types/bun": "^1.0.12",
    "@types/react": "^18.2.74",
    "@types/react-dom": "^18.2.21",
    "autoprefixer": "latest",
    "drizzle-kit": "^0.20.14",
    "tailwindcss": "latest",
    "typescript": "^5.4.4",
    "wrangler": "3.48.0"
  }
}
