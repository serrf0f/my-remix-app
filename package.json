{
  "name": "remix-app-cloudflare",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "clean": "rm -Rf node_modules/ build/ .cache/",
    "db:generate": "drizzle-kit generate:sqlite",
    "db:dev": "wrangler d1 execute DB --local",
    "db:preview": "wrangler d1 execute DB --preview",
    "db:production": "wrangler d1 execute DB",
    "predeploy": "bun run build",
    "deploy": "wrangler pages deploy ./build/client",
    "dev": "remix vite:dev --force",
    "format": "bun biome format --write ./app ./*.ts",
    "lint": "bun biome check --apply ./app",
    "start": "wrangler pages dev ./build/client",
    "typecheck": "tsc",
    "typegen": "wrangler types"
  },
  "dependencies": {
    "@lucia-auth/adapter-drizzle": "1.1.0",
    "@lucia-auth/adapter-sqlite": "3.0.2",
    "@marsidev/react-turnstile": "^0.7.2",
    "@radix-ui/react-avatar": "^1.1.0",
    "@radix-ui/react-checkbox": "^1.1.1",
    "@radix-ui/react-dialog": "^1.1.1",
    "@radix-ui/react-dropdown-menu": "^2.1.1",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-popover": "^1.1.1",
    "@radix-ui/react-progress": "^1.1.0",
    "@radix-ui/react-scroll-area": "^1.1.0",
    "@radix-ui/react-select": "^2.1.1",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-switch": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.0",
    "@radix-ui/react-tooltip": "^1.1.2",
    "@react-email/components": "0.0.22",
    "@react-email/render": "^0.0.17",
    "@remix-run/cloudflare": "^2.11.1",
    "@remix-run/cloudflare-pages": "^2.11.1",
    "@remix-run/react": "^2.11.1",
    "@remix-run/serve": "^2.11.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.0",
    "date-fns": "^3.6.0",
    "drizzle-orm": "^0.33.0",
    "isbot": "^5.1.14",
    "lucia": "3.2.0",
    "lucide-react": "^0.427.0",
    "react": "18.3.1",
    "react-day-picker": "^9.0.8",
    "react-dom": "18.3.1",
    "recharts": "^2.12.7",
    "tailwind-merge": "^2.5.2",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^0.9.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "@cloudflare/workers-types": "^4.20240806.0",
    "@remix-run/dev": "^2.11.1",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "latest",
    "drizzle-kit": "^0.24.0",
    "node-fetch": "^3.3.2",
    "tailwindcss": "latest",
    "typescript": "^5.5.4",
    "vite": "^5.4.0",
    "vite-tsconfig-paths": "^5.0.1",
    "wrangler": "3.70.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
